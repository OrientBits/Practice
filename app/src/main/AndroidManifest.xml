<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lequiz.practice">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CurrentAffairsActivity"
            android:label="@string/current_Affairs"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".ComputerActivity"
            android:label="@string/computer"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".MathematicsActivity"
            android:label="@string/mathematics"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".ReasoningActivity"
            android:label="@string/reasoning"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".ScienceActivity"
            android:label="@string/general_science"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".EnglishActivity"
            android:label="@string/english"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".EntertainmentActivity"
            android:label="@string/entertainment"
            android:parentActivityName=".HomeActivity" />
        <activity android:name=".SportActivity" />
        <activity android:name=".TechnologyActivity" />
        <activity android:name=".SpecialActivity"></activity>
    </application>

</manifest>