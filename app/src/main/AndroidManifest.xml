<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.lequiz.practice">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".module.Practise"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity
            android:name=".DispatcherActivity"
            android:theme="@style/AppTheme.Launcher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".home.HomeContainer"
            android:screenOrientation="portrait" />
        <activity android:name=".OnBoarding" />
        <activity
            android:name=".category.MathematicsActivity"
            android:label="@string/mathematics"
            android:parentActivityName=".category.MathematicsActivity" />
        <activity
            android:name=".category.ReasoningActivity"
            android:label="@string/reasoning"
            android:parentActivityName=".category.ReasoningActivity" />
        <activity
            android:name=".category.EntertainmentActivity"
            android:label="@string/entertainment"
            android:parentActivityName=".category.EntertainmentActivity" />
        <activity
            android:name=".category.TechnologyActivity"
            android:label="@string/technology"
            android:parentActivityName=".category.TechnologyActivity" />
        <activity
            android:name=".category.CurrentAffairsActivity"
            android:label="@string/current_Affairs"
            android:parentActivityName=".category.CurrentAffairsActivity" />
        <activity
            android:name=".category.ComputerActivity"
            android:label="@string/computer"
            android:parentActivityName=".category.ComputerActivity" />
        <activity
            android:name=".category.GeneralScienceActivity"
            android:label="@string/general_science"
            android:parentActivityName=".category.GeneralScienceActivity" />
        <activity
            android:name=".category.EnglishActivity"
            android:label="@string/english"
            android:parentActivityName=".category.EnglishActivity" />
        <activity
            android:name=".category.SportsActivity"
            android:label="@string/sports"
            android:parentActivityName=".category.SportsActivity" />
        <activity
            android:name=".category.SpecialActivity"
            android:label="@string/special"
            android:parentActivityName=".category.SpecialActivity" />
        <activity
            android:name=".nav_drawer.ProfileActivity"
            android:parentActivityName=".nav_drawer.ProfileActivity" />
        <activity
            android:name=".nav_drawer.NavLeaderboard"
            android:label="@string/leaderboard"
            android:parentActivityName=".nav_drawer.NavLeaderboard" />
        <activity
            android:name=".nav_drawer.NavNotifications"
            android:label="@string/notifications"
            android:parentActivityName=".nav_drawer.NavNotifications" />
        <activity
            android:name=".nav_drawer.NavPayment"
            android:label="@string/payment"
            android:parentActivityName=".nav_drawer.NavPayment" />
        <activity
            android:name=".nav_drawer.NavSettings"
            android:label="@string/settings"
            android:parentActivityName=".nav_drawer.NavSettings" />
        <activity
            android:name=".nav_drawer.NavInviteFriends"
            android:label="@string/invite_friends"
            android:parentActivityName=".nav_drawer.NavInviteFriends"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name=".nav_drawer.QuizFactory"
            android:label="@string/quiz_factory"
            android:parentActivityName=".nav_drawer.QuizFactory" />
        <activity
            android:name=".RegisterUser"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".Login"
            android:label="@string/title_activity_login"
            android:theme="@style/AppTheme"
            android:windowSoftInputMode="adjustResize" />
        <activity android:name=".module.WebViewLayout" />
        <activity android:name="com.soundcloud.android.crop.CropImageActivity" />
        <activity android:name=".nav_drawer.GenderChooser" />
        <activity android:name=".EditProfile"></activity>
    </application>

</manifest>